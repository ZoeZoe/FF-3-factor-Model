  {
   "cell_type": "code",
   "execution_count": 36,
   "metadata": {},
   "outputs": [],
   "source": [
    "ff_pos_mm = Med_pos_mm.loc[:,['Mkt-RF', 'SMB', 'HML', 'RMW', 'CMA']]\n",
    "ff_neg_mm = Med_neg_mm.loc[:,['Mkt-RF', 'SMB', 'HML', 'RMW', 'CMA']]\n",
    "models_ff_pos_mm = []\n",
    "models_ff_neg_mm = []\n",
    "for i in range (5):\n",
    "    x = ff_pos_mm.drop(ff_pos_mm.columns[i], axis=1)\n",
    "    x = sm.add_constant(x)\n",
    "    y = ff_pos_mm.iloc[:, i]\n",
    "    reg = sm.OLS(y, x)   \n",
    "    models_ff_pos_mm.append(reg.fit())\n",
    "for i in range (5):\n",
    "    x = ff_neg_mm.drop(ff_neg_mm.columns[i], axis=1)\n",
    "    x = sm.add_constant(x)\n",
    "    y = ff_neg_mm.iloc[:, i]\n",
    "    reg = sm.OLS(y, x)   \n",
    "    models_ff_neg_mm.append(reg.fit())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 864x504 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
